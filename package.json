        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "date-fns": "^3.6.0",
        "date-fns-tz": "^3.2.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
