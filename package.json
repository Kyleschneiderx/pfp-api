{
    "name": "pelvicfloor",
    "version": "1.0.1",
    "engines": {
        "node": ">=12.9.0"
    },
    "description": "",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
        "test:fresh": "export APP_ENV=testing && npm run db:reset && node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "lint": "eslint src",
        "start:dev": "nodemon -L --watch ./src src/index.js",
        "start": "npm run db:migrate && npm run db:seed && node src/index.js",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
    },
    "author": "",
    "license": "ISC",
